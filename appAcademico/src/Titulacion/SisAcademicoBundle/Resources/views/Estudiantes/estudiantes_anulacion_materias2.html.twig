{% if (banderaMatricula==4 )%}

    <div class="col-lg-12">
            <b>{{ estudiante }}</b> Usted se encuentra matriculado en las siguientes materias:
            <table class="table table-striped table-bordered" id ="">
                     <thead>
                        <tr>
                                <th colspan="7" style="text-align: center !important;background-color: #337AB7 !important;color: white!important;">Periodo  {{ cicloencurso }}</th>
                        </tr>
                        <tr>
                            <th style="text-align: center !important;">Nivel</th>
                            <th style="text-align: center !important;">Id Materias</th>
                            <th style="text-align: center !important;">Nombre Materias</th>
                            <th style="text-align: center !important;">Veces</th>
                            <th style="text-align: center !important;">Curso</th> 
                            <th style="text-align: center !important;"></th> 
                            <th style="text-align: center !important;"></th> 
                        </tr>
                        {% for dato in Materias_inscribir %} 
                            <tr>
                                <td align="center">{{ dato.Nivel }}</td>
                                <td align="center">{{ dato.IdMateria }}</td>
                                <td align="center">{{ dato.Nombre }}</td>
                                <td align="center">{{ dato.Veces }}</td>
                                <td align="center">{{ dato.Curso }}</td>
                                <td align="center"><input type="button" name="btn_{{ dato.IdMateria }}" id="btn_{{ dato.IdMateria }}" value="Anular" /></td>
                                <td align="center"><div id="contenedor_{{ dato.IdMateria }}"></div></td>
                            </tr>
                        {% endfor %}
                <tfoot>
                    <tr>
                        <td colspan=5 align="right">
                            <!--input type="submit" name="Generapdf" id="btngenpdfmat" value="Imprimir Documento"></td-->
                        </td>
                    </tr>
                </tfoot>
               
           </table>
        </div>


        <script>
        {% for dato in Materias_inscribir %} 
            $("#btn_{{ dato.IdMateria }}").on("click",function(event){
                    var materiaAnulada = $('#contenedor_{{ dato.IdMateria }}');
                     //var materiaAnuladaBoton = $('#btn_{{ dato.IdMateria }}');
                    var idEstudiante='{{idEstudiante}}';
                    var idCarrera='{{idCarrera}}';
                    var idCiclo='{{idciclo}}';
                    var idMateria='{{ dato.IdMateria }}';
                    var obj_data = {'idEstudiante': idEstudiante,'idCarrera':idCarrera,'idCiclo':idCiclo,'idMateria':idMateria};

                     var jumpAction = '{{path('estudiantes_anulacion_materias_2')}}';
                      $.ajax({
                         url:   jumpAction,
                         data: obj_data,
                         type:  'post',
                         async: true,
                         beforeSend: function () {
                            materiaAnulada.html("Cargando, espere por favor...");
                            //materiaAnuladaBoton.css("display","none")
                         },
                         success:  function (dataResponse) {
                             materiaAnulada.html(dataResponse);
                         },
                          error: function(dataResponse)
                          {
                            form="accordion";
                            msg_alert = alert_bootstrap( form, 'Atenci&oacute;n', "Nos encontramos en mantenimiento de nuestros servidores, por favor intenta nuevamente m&aacute;s tarde.", 'sm', 'alert');
                                            $( '#'+form ).append( msg_alert );
                                            $('#modal-'+form).modal('show');
                          },
                

                         dataType:'html'
                      });



                     var doc = new jsPDF();
                     doc.text(20, 20, 'Hello world!');
                     doc.text(20, 30, 'This is client-side Javascript, pumping out a PDF.');
                     doc.addPage();
                     doc.text(20, 20, 'Do you like that?');

                         // Output as Data URI
                         doc.output('datauri');

               }); 
         
        {% endfor %}
        </script>
{% endif %}

            

